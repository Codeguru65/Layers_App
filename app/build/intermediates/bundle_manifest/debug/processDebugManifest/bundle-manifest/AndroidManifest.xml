<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.layers"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="29" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.layers.SignUpActivity"
            android:resizeableActivity="true"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name="com.example.Views.Egg_History" />
        <activity android:name="com.example.Views.Stock" />
        <activity android:name="com.example.Views.Daily_Entries" />
        <activity android:name="com.example.layers.edit_feed_inventory" />
        <activity
            android:name="com.example.layers.FeedActivity"
            android:resizeableActivity="true"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name="com.example.layers.DailyActivityViewer" />
        <activity
            android:name="com.example.deliveries.PartPay"
            android:resizeableActivity="true"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name="com.example.deliveries.Account" />
        <activity android:name="com.example.deliveries.Cash" />
        <activity
            android:name="com.example.layers.Water"
            android:resizeableActivity="true" />
        <activity
            android:name="com.example.layers.Mort"
            android:resizeableActivity="true" />
        <!--
 <activity
            android:name=".Submenu"
            android:resizeableActivity="true" />
        -->
        <activity
            android:name="com.example.layers.LoginActivity"
            android:resizeableActivity="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.layers.DailyFeedActivity"
            android:resizeableActivity="true" />
        <activity
            android:name="com.example.layers.MainActivity"
            android:resizeableActivity="true" />
        <activity
            android:name="com.example.layers.EggP"
            android:resizeableActivity="true" />
        <activity
            android:name="com.example.layers.Health"
            android:resizeableActivity="true" />
        <activity
            android:name="com.example.production.Inventory"
            android:resizeableActivity="true" />

        <receiver
            android:name="com.example.Notifications.AlarmReceiver"
            android:enabled="true" />

        <service
            android:name="com.example.Notifications.NotificationService"
            android:enabled="true" />
        <service
            android:name="androidx.room.MultiInstanceInvalidationService"
            android:exported="false" />

        <provider
            android:name="androidx.lifecycle.ProcessLifecycleOwnerInitializer"
            android:authorities="com.example.layers.lifecycle-process"
            android:exported="false"
            android:multiprocess="true" />
    </application>

</manifest>